<Window x:Class="ThisGym.ReassignSlaveWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThisGym"
        mc:Ignorable="d"
        Title="ReassignSlaveWin" SizeToContent="WidthAndHeight">
    <Canvas Height="240" Width="451">
        <DataGrid x:Name="GymMasters_DG" HorizontalAlignment="Left" Height="157" VerticalAlignment="Top" Width="437" Canvas.Left="7" Canvas.Top="33"/>
        <Label Content="Список тренеров:" HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Padding="1,3,5,5" Width="437" FontSize="14" FontFamily="Arial Black" Canvas.Left="7" Canvas.Top="10"/>
        <Separator HorizontalAlignment="Left" Height="14" Margin="0" VerticalAlignment="Top" Width="451" Canvas.Top="190"/>
        <Button x:Name="Cancel_B" Content="Отмена" Canvas.Left="10" Canvas.Top="204" Width="124" Height="26" Click="Cancel_B_Click"/>
        <Button x:Name="ReassignSlave_B" Content="Подтвердить" Canvas.Left="317" Canvas.Top="204" Width="124" Height="26" Click="ReassignSlave_B_Click"/>

    </Canvas>
</Window>
